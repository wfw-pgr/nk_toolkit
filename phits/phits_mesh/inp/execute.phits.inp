$$ ---------------------------------------------------------- $$
$$ --- tally for fluence on tetra                         --- $$
$$ ---------------------------------------------------------- $$
[T-Track]
   mesh =  tet          $$ mesh    :: [ xyz, r-z, reg, tet ]
    reg =  101          $$ region number ::
 e-type =    2
     ne =    1
   emin =  0.0
   emax =  1.0e3
   part =  photon
   unit =    2           $$ unit is [1/cm^2/MeV/source]
   axis =  tet
   file =  out/fluence_tet.dat
 foamout=    2

$$ ---------------------------------------------------------- $$
$$ --- tally for fluence x-z (2d)                         --- $$
$$ ---------------------------------------------------------- $$
[T-Track]
   mesh =  xyz          $$ mesh    :: [ xyz, r-z, reg, tet ]
 x-type =    2          $$ x-type  :: [ 1: (nx,data[nx+1]), 2:(nx,xmin,xmax), 3:logarithmic ]
     nx =   100
   xmin =  -16
   xmax =  +16
 y-type =    2
     ny =    1
   ymin =  -8
   ymax =  +8
 z-type =    2
     nz =   100
   zmin =  -2
   zmax =  +2
 e-type =    2
     ne =    1
   emin =  0.0
   emax =  1.0e3
   axis =   xz
   part =  all
   file =  out/fluence_xz.dat
  title =  fluence in xz plane
  gshow =    1
 epsout =    1




$$ ---------------------------------------------------------- $$
$$ --- tally for heat deposit on tetra                    --- $$
$$ ---------------------------------------------------------- $$
[T-Deposit]
    file = out/heatload.out
    mesh = tet
    reg  = 101    $$ region of tetra is defined.
    unit = 5      $$ [J/m3/soruce] ( output=dose only )
                  $$ ( J/m2 in manual7.5, but J/m3 in PHITs-Fluent )
    axis = tet
  output = dose
 foamout = 2      $$ out:: .csv ( i_tet, x0,y0,z0, vol, value, stddev )




$$ ---------------------------------------------------------- $$
$$ --- tally for geometry check ( target region )          ---$$
$$ ---------------------------------------------------------- $$
[T-Track]
   mesh =  xyz          $$ mesh    :: [ xyz, r-z, reg, tet ]
 x-type =    2          $$ x-type  :: [ 1: (nx,data[nx+1]), 2:(nx,xmin,xmax), 3:logarithmic ]
     nx =   800
   xmin =  -30.0
   xmax =  30.0
 y-type =    2
     ny =    1
   ymin =  -30.0
   ymax =  30.0
 z-type =    2
     nz =  1200
   zmin =  -30.0
   zmax =  30.0
 e-type =    2
     ne =    1
   emin =  0.0
   emax =  1.0e3
   axis =   xz
   file =  out/check_geometry.dat
  title =  geometry of target region
  gshow =    1
 epsout =    1



[End]


$$ ---   [NOTE]    --------------------------------------------------------------  $$
$$ nucdata   =  1                       $$ nuetron's nuclear data library => 0:Off, 1:On
$$ negs      =  1                       $$ transports of electron, photon, position ( default:-1 )
                                        $$  -1: PHITS's original library ( photon )
                                        $$   0: No Transport
                                        $$  +1: EGS5 Library             ( photon, electron )
$$ emcpf     = 20.0                     $$ upper limit of the detailed photon model
$$ emin(12)  = 0.1     $ ==  1(keV)     $$ upr-limit of transport :: electron (MeV)
$$ emin(13)  = 0.1     $ ==  1(keV)     $$ upr-limit of transport :: positron (MeV)
$$ dmax(12)  = 1000.0                   $$ use of nuclear data ( emin(12) < energy < dmax(12) )
